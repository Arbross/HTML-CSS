<Window x:Class="UI_App.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UI_App"
        mc:Ignorable="d"
        Title="MainWindow" Height="900" Width="1600">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <DataGrid Name="One" Grid.Column="0"/>
        <DataGrid Name="Two" Grid.Column="1"/>
        <DataGrid Name="Three" Grid.Column="2"/>
        <DataGrid Name="Four" Grid.Column="3"/>
        <DataGrid Name="Five" Grid.Column="4"/>
        <DataGrid Name="Six" Grid.Column="5"/>
        <DataGrid Name="Seven" Grid.Column="6"/>
    </Grid>
</Window>
